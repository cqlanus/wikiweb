{
  "manifest_version": 2,
  "name": "Wiki Web",
  "description": "This extension shows collects and displays wikipedia history",
  "version": "1.0.0",
  "key": "",
  "icons": {
    "128": "assets/rainbow.png"
  },

  "page_action" : {
    "default_title": "Wiki Web - default title",
    "default_icon" : "assets/rainbow.png",
    "default_popup": "views/popup.html"
  },


  "permissions": [
    "identity",
    "http://*/",
    "https://accounts.google.com/*",
    "https://www.googleapis.com/*",
    "https://d3js.org/d3.v4.min.js",
    "activeTab",
    "tabs",
    "https://ajax.googleapis.com/",
    "https://docs.google.com/feeds/*",
    "https://www.google.com/accounts/OAuthGetRequestToken",
    "https://www.google.com/accounts/OAuthAuthorizeToken",
    "https://www.google.com/accounts/OAuthGetAccessToken"
  ],

  "background": {
    "scripts" : ["js/jquery-3.2.1.min.js", "js/d3.min.js",  "js/background.js"],
    "persistent" : false
  },
  "oauth2": {
    "client_id":"867227488481-1co857f1fgtt5f8np7q2oheb47jcrnp4.apps.googleusercontent.com",
    "scopes": ["https://www.googleapis.com/auth/plus.login"]
  },

  "content_scripts": [
    {
        "matches": ["https://en.wikipedia.org/wiki/*"],
        "js": ["js/content.js"]
    }]
  }


//"./js/chrome_ex_oauthsimple.js", "./js/chrome_ex_oauth.js","./js/onload.js"
